{
  "name": "front-i18n",
  "displayName": "Vue 国际化开发助手",
  "description": "快速为中文 Vue 项目添加国际化支持",
  "keywords": [
    "vue",
    "front",
    "前端",
    "i18n",
    "国际化",
    "多语言",
    "helper"
  ],
  "version": "0.0.2",
  "publisher": "hancel",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/logo.png",
  "main": "./out/extension.js",
  "license": "MIT",
  "repository": "https://github.com/imlinhanchao/vsc-vue-i18n",
  "activationEvents": [
    "onCommand:front-i18n.autoRecognition"
  ],
  "contributes": {
    "commands": [
      {
        "command": "front-i18n.autoRecognition",
        "title": "自动识别中文",
        "icon": "$(vue-auto-recognition)"
      },
      {
        "command": "front-i18n.addText",
        "title": "添加到国际化"
      },
      {
        "command": "front-i18n.removePosition",
        "title": "移除翻译位置"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "front-i18n.autoRecognition",
          "group": "navigation",
          "when": "editorLangId == vue || editorLangId == javascript || editorLangId == typescript"
        }
      ]
    },
    "icons": {
      "vue-auto-recognition": {
        "description": "自动识别",
        "default": {
          "fontPath": "assets/icons/icon.woff2",
          "fontCharacter": "\\e661"
        }
      }
    },
    "configuration": {
      "title": "Vue 国际化开发助手",
      "properties": {
        "front-i18n.appId": {
          "type": "string",
          "default": "",
          "markdownDescription": "百度翻译接口 appId，若不设置则无法自动生成 Key 与自动翻译导出结果。申请方法见[百度翻译文档](http://api.fanyi.baidu.com/product/113)(有免费额度)"
        },
        "front-i18n.appKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "百度翻译接口 API Key，若不设置则无法自动生成 Key 与自动翻译导出结果。申请方法见[百度翻译文档](http://api.fanyi.baidu.com/product/113)(有免费额度)"
        },
        "front-i18n.i18nFunctionName": {
          "type": "string",
          "default": "$t",
          "description": "插入到代码的 i18n 函数名"
        },
        "front-i18n.autoTranslateResult": {
          "type": "boolean",
          "default": false,
          "description": "是否自动翻译导出结果"
        },
        "front-i18n.exportLanguageExcel": {
          "type": "boolean",
          "default": false,
          "description": "是否导出翻译结果 Excel"
        },

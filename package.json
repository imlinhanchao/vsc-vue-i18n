    "compile": "cd webview && npm run build && cd .. && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "dev": "cd webview && npm run dev"
